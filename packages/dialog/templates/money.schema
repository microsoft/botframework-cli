{
    "$schema": "http://json-schema.org/draft-07/schema",
    "money": {
        "type": "object",
        "$entities": ["${concat('money:', property)}", "money"],
        "properties": {
            "number": {
                "type": "number"
            },
            "units": {
                "type": "string"
            }
        },
        "required": ["number", "units"],
        "$templates": ["money.lu","objectProperty.lg", "string-missing.dialog","assign-moneyEntity.dialog"]
    }
}