{
    "$schema": "http://json-schema.org/draft-07/schema",
    "datetime": {
        "type": "object",
        "$entities": ["@{concat('datetime:', property)}", "datetime"],
        "properties": {
            "type": {
                "type": "string"
            },
            "timex": {
                "type": "string"
            }
        },
        "required": ["type", "timex"],
        "$templates": ["datetime.lu","objectProperty.lg", "stringAsk.dialog","datetimeSet.dialog"]
    }
}