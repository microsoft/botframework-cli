{
    "$schema": "http://json-schema.org/draft-07/schema",
    "age": {
        "type": "object",
        "$mappings": ["age"],
        "properties": {
            "number": {
                "type": "number"
            },
            "units": {
                "type": "string"
            }
        },
        "required": ["number", "units"],
        "$templates": ["age.lu","age.lg", "stringAsk.dialog","ageSet.dialog"]
    }
}