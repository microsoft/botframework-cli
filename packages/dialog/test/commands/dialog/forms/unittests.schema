{
    "$schema": "http://json-schema.org/draft-07/schema",
    "type": "object",
    "properties": {      
        "Personname": {
            "type": "string",
            "$mappings": [
                "personName"
            ]
        },
        "Age": {
            "$ref": "template:age.schema#/age"
        },
        "Ordinal": {
            "type": "string",
            "$mappings": [
                "ordinal"
            ]
        },
        "Geographyv2": {
            "$ref": "template:geography.schema#/geographyV2"
        },
        "Keyphraseproperty": {
            "type": "string",
            "$mappings": [
                "keyPhrase"
            ]
        },
        "Money": {
            "$ref": "template:money.schema#/money"
        },
        "Percentage": {
            "type": "string",
            "$mappings": [
                "percentage"
            ]
        },
        "Temperature": {
            "$ref": "template:temperature.schema#/temperature"
        },
        "Url": {
            "type": "string",
            "$mappings": [
                "url"
            ]
        },       
        "Quantity": {
            "type": "number",
            "$mappings": [
                "number"
            ],
            "minimum": 1,
            "maximum": 10
        },
        "Utteranceproperty": {
            "type": "string",
            "$mappings": [
                "utterance"
            ],
            "$comment": "This indicates the whole string is what maps to this."
        },
        "Dimension": {
            "$ref": "template:dimension.schema#/dimension"
        },
        "Enum1": {
            "type": "string",
            "enum": [
                "multiGrainWheat",
                "rye",
                "white",
                "wholeWheat"
            ]
        },
        "Enum2": {
            "type": "string",
            "enum": [
                "american",
                "cheddar",
                "feta",
                "gouda",
                "pepper jack",
                "provolone",
                "swiss",
                "none"
            ]
        },
        "Zipcodepattern": { 
            "type": "string",
            "pattern": "^[0-9]{5}(?:-[0-9]{4})?$",
            "$mappings": ["utterance","number"]
        },
        "Email": {
            "type": "string",
            "$mappings": ["email"]
        }
    },
    "required": 
         ["Personname", "Age", "Ordinal", "Geographyv2", "Keyphraseproperty", "Money", "Percentage", "Temperature", "Url", "Quantity", "Utteranceproperty", "Dimension", "Enum1", "Enum2", "Zipcodepattern","Email"],
    "$requires": [
        "standard.schema"
    ]
}