        },
        "entities": {
          "type": "array",
          "title": "Entities",
          "description": "Required entities.",
          "items": {
            "type": "string",
            "title": "Entity",
            "description": "Entity that must be present."
          }
        },
        "condition": {
          "$ref": "#/definitions/condition",
          "title": "Condition",
          "description": "Condition (expression).",
          "examples": [
