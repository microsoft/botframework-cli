{
  "name": "@microsoft/bf-chatdown",
  "description": "Tool for parsing chat files and outputting replayable activities",
  "version": "1.0.0",
  "author": "Microsoft",
  "bugs": "https://github.com/microsoft/botframework-cli",
  "dependencies": {
    "@microsoft/bf-cli-command": "1.0.0",
    "@oclif/command": "~1.8.36",
    "@oclif/config": "~1.13.3",
    "@oclif/errors": "~1.2.2",
    "botframework-schema": "^4.22.3",
    "chalk": "2.4.1",
    "cli-table3": "^0.5.1",
    "fs-extra": "^5.0.0",
    "glob": "^7.1.3",
    "intercept-stdout": "^0.1.2",
    "latest-version": "5.1.0",
    "mime-types": "^2.1.18",
    "minimist": "^1.2.6",
    "axios": "~1.6.0",
    "https-proxy-agent": "^5.0.0",
    "please-upgrade-node": "^3.0.1",
    "semver": "^5.5.1",
    "tslib": "^2.0.3",
    "window-size": "^1.1.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.0",
    "@oclif/plugin-help": "^2.1.6",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/fs-extra": "^5.0.5",
    "@types/mocha": "^10.0.6",
    "@types/node": "^11.3.7",
    "@types/rimraf": "^2.0.2",
    "chai": "^4.4.1",
    "globby": "^11.0.4",
    "nyc": "^15.1.0",
    "mocha": "^10.4.0",
    "rimraf": "^2.6.3",
    "ts-node": "^10.8.1",
    "tslint": "^5.16.0",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/lib",
    "/utils",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/microsoft/botframework-cli",
  "keywords": [
    "oclif-plugin"
  ],
  "main": "utils/index.js",
  "license": "MIT",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "bf",
    "devPlugins": [
      "@oclif/plugin-help"
    ]
  },
  "mocha": {
    "extension": [
      ".test.js",
      ".test.ts"
    ],
    "recursive": true,
    "timeout": 10000,
    "require": [
      "../../test/hook.js",
      "source-map-support/register"
    ]
  },
  "repository": "https://github.com/microsoft/botframework-cli/tree/master/packages/chatdown",
  "scripts": {
    "prepack": "npm run clean && npm run build && npm run doc:readme",
    "postpack": "rimraf oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "build": "tsc -b",
    "clean": "rimraf ./.nyc_output ./lib ./package-lock.json ./tsconfig.tsbuildinfo",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "doc": "npm run build && npm run doc:readme && rimraf oclif.manifest.json",
    "doc:readme": "oclif-dev manifest && oclif-dev readme",
    "version": "npm run doc:readme && git add README.md"
  }
}
