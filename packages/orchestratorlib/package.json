{
  "name": "@microsoft/bf-orchestrator",
  "description": "APIs to interact with BF Orchestrator.",
  "version": "1.0.0",
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "cpu": [
    "x64",
    "ia32"
  ],
  "engines": {
    "node": ">=10"
  },
  "author": "Microsoft",
  "bugs": "https://github.com/microsoft/botframework-cli/issues",
  "main": "lib/index.js",
  "files": [
    "/lib"
  ],
  "homepage": "https://github.com/microsoft/botframework-cli",
  "license": "MIT",
  "mocha": {
    "extension": [
      ".test.js",
      ".test.ts"
    ],
    "recursive": true,
    "timeout": 60000,
    "require": [
      "../../test/hook.js",
      "source-map-support/register"
    ]
  },
  "repository": "https://github.com/microsoft/botframework-cli/tree/master/packages/orchestratorlib",
  "scripts": {
    "prepack": "npm run clean && npm run build",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "build": "tsc -b",
    "clean": "rimraf ./.nyc_output ./lib ./package-lock.json ./tsconfig.tsbuildinfo",
    "test": "mocha",
    "coverage": "nyc npm run test",
    "doc": "",
    "doc:readme": "",
    "version": "npm run doc:readme && git add README.md"
  },
  "dependencies": {
    "@microsoft/bf-lu": "1.0.0",
    "@microsoft/bf-dispatcher": "1.0.0",
    "axios": "~1.6.0",
    "https-proxy-agent": "^5.0.0",
    "tslib": "^2.0.3",
    "@microsoft/orchestrator-core": "~4.15.1",
    "@types/fs-extra": "~8.1.0",
    "fs-extra": "~9.0.0",
    "read-text-file": "~1.1.0",
    "unzip-stream": "^0.3.4",
    "fast-text-encoding": "^1.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^11.13.7",
    "@types/sinon": "^9.0.4",
    "chai": "^4.4.1",
    "eslint": "^5.16.0",
    "mocha": "^10.4.0",
    "nock": "^11.7.0",
    "nyc": "^15.1.0",
    "rimraf": "^2.6.3",
    "ts-node": "^10.8.1",
    "typescript": "^4.9.5",
    "sinon": "^9.0.2"
  }
}
