    "https-proxy-agent": "^2.2.1",
    "intercept-stdout": "^0.1.2",
    "md5": "^2.2.1",
    "node-fetch": "^2.1.2",
    "pascal-case": "^2.0.1",
    "readline": "^1.3.0",
    "readline-sync": "^1.4.9",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.3",
    "semver": "^5.5.1",
    "tslib": "^1.10.0",
    "window-size": "^1.1.0"
  },
