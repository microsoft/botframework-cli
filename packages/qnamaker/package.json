    "@oclif/dev-cli": "^1.22.0",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/test": "^1.2.4",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.1.7",
    "@types/mocha": "^5.2.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^10.14.13",
    "chai": "^4.2.0",
    "fetch-mock": "^7.3.9",
    "globby": "^10.0.1",
    "mocha": "^6.2.2",
    "nock": "^11.7.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.3.0",
    "tslint": "^5.18.0",
    "rimraf": "^2.6.3",
    "typescript": "^3.5.3"
  }
