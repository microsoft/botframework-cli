{
  "extension": [
    ".ts",
    ".js"
  ],
  "include": [
    "src"
  ],
  "exclude": [
    "**/node_modules/**",
    "**/tests/**",
    "**/coverage/**",
    "**/*.d.ts"
  ],
  "reporter": [
    "lcov",
    "text"
  ],
  "all": true,
  "cache": true,
  "extends": "@istanbuljs/nyc-config-typescript",
  "check-coverage": false
}
